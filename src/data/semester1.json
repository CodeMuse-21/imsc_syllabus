    {
      "code": "MA101",
      "title": "Calculus-I",
      "credits": "3-1-0",
      "modules": {
        "Module I": [
          "Successive Differentiation, Leibnitz Theorem.",
          "Mean Value Theorem: Rolle's, Lagrange's and Cauchy's MVT.",
          "Taylor & Maclaurin expansions.",
          "Increasing/decreasing functions, concavity, inflection point, extrema."
        ],
        "Module II": [
          "Curve analysis: Asymptotes, orthogonal curves.",
          "Curvature and radius of curvature in Cartesian, parametric and polar forms."
        ],
        "Module III": [
          "Functions of several variables, limits, partial derivatives.",
          "Euler's theorem, implicit and composite derivatives.",
          "Jacobian, Taylor series for multivariable functions.",
          "Maxima/minima of several variables, Lagrange multipliers."
        ],
        "Module IV": [
          "Definite integral, reduction formula.",
          "Differentiation under the integral sign (Leibniz rule).",
          "Improper integrals, convergence, Beta & Gamma functions."
        ],
        "Module V": [
          "Applications: arc length, areas, volumes, solids of revolution, moments."
        ]
      },
      "resources": {
        "youtube": "https://www.youtube.com/playlist?list=PLLy_2iUCG87CNafffzNZPVa9rW-QrkOvd",
        "notes": "https://www.math.brown.edu/johsilva/Calculus.pdf",
        "question_papers": "https://drive.google.com/drive/folders/1YfW1FhX8O72C7JgYJxxlH0nV9CG-Prev",
        "books": "https://archive.org/details/CalculusThomas13",
        "extra": "https://ocw.mit.edu/courses/18-01sc-single-variable-calculus-fall-2010/"
      }
    },

